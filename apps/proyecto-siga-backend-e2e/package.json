{
  "name": "@proyecto-siga/proyecto-siga-backend-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@proyecto-siga/proyecto-siga-backend"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/proyecto-siga-backend-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@proyecto-siga/proyecto-siga-backend:build",
          "@proyecto-siga/proyecto-siga-backend:serve"
        ]
      }
    }
  }
}
